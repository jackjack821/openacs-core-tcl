<?xml version="1.0"?>

<queryset>
   <rdbms><type>oracle</type><version>8.1.6</version></rdbms>

<fullquery name="all_subprograms">      
      <querytext>

	select proname as name, 'function' as type 
	from pg_proc 
	where proowner=(select usesysid from pg_shadow 
	                where usename = current_user) 
	order by proname
    
      </querytext>
</fullquery>

 
</queryset>
